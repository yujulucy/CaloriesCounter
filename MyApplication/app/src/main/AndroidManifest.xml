<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="sg.edu.nus.myapplication">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CHANGE_NETWORK_STATE" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@drawable/logo2"
        android:label="Food Diary"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".UserAreaActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".UpdateDeleteData" />
        <activity android:name=".AddFoodDataActivity" />
        <activity android:name=".ReadDataActivity" />
        <activity android:name=".FoodDiaryActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".CalendarActivity"/>
        <activity android:name=".Calculator2"></activity>
    </application>

</manifest>