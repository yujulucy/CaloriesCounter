<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="sg.edu.nus.myapplication">

    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo2"
        android:label="Calorie Counter SG"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RegisterActivity"/>
        <activity android:name=".UserAreaActivity"/>
        <activity android:name=".NewFoodActivity" />
        <activity android:name=".NewFoodAddedActivity" />
        <activity android:name=".Calculator" />
        <activity android:name=".ListActivity" />
        <activity android:name=".DatabaseActivity" />
        <activity android:name=".FoodListActivity"></activity>
    </application>

</manifest>